{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/auth/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/chats/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/me/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/meta-prompts/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/settings/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/admin/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/images/*",
      "/*.{css,js,json,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot,webp}"
    ]
  }
}
